  "scripts": {
    "start": "npm-run-all --parallel build:dev dev",
    "start:limitmem": "npm-run-all --parallel build:dev:limitmem dev",
    "start:debug": "npm-run-all --parallel open:client debug",
    "start:prod": "npm-run-all --parallel build:prod prod",
    "open:client": "ng serve --open",
    "build": "npm run build:prod",
    "build:dev": "ng build --watch --aot",
    "build:dev:limitmem": "node --max_old_space_size=1024 ./node_modules/@angular/cli/bin/ng build --watch --aot",
    "build:prod": "ng build --prod",
    "build:aot": "ng build --aot",
    "clean": "rimraf assets/dist",
    "prod": "node app.js --prod",
    "dev": "node app.js -- --dev",
    "debug": "node --inspect app.js"
  },
  "main": "app.js",
